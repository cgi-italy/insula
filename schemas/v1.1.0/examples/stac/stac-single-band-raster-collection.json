{
  "type": "Collection",
  "id": "stac-single-band-raster-collection",
  "title": "Single band raster collection",
  "stac_extensions": [
    "https://insula.earth/schemas/v1.1.0/stac/insula-stac-extension.schema.json"
  ],
  "insula:metadata": {
    "version": "v1.1.0",
    "type": "raster",
    "bands": [
      {
        "name": "NDVI",
        "description": "Normalized difference vegetation index",
        "domain": {
          "min": -1,
          "max": 1,
          "noData": -9999
        },
        "quantity": {
          "id": "ndvi",
          "name": "NDVI"
        },
        "default": {
          "colorScale": "viridis",
          "range": {
            "min": 0,
            "max": 1
          },
          "clamp": true
        }
      }
    ]
  }
}
